@model IEnumerable<BAI.Adir.Api.Domain.Model.ContentFileType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Folder)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Folder)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContentFileTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ContentFileTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContentFileTypeId })
        </td>
    </tr>
}

</table>
